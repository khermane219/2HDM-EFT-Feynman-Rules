(* Functions replace variables when expansion is called up to order 1/LambdaCutoff^2 *)

ee[LambdaCutoff_] := g1 gw (1 - Hold[xWB g1 gw / (g1^2 + gw^2)] / LambdaCutoff^2) / Sqrt[g1^2 + gw^2];
ee[LambdaCutoff_] := g1 gw (1 - Hold[xWB g1 gw / (g1^2 + gw^2)] / LambdaCutoff^2) / Sqrt[g1^2 + gw^2];

mA2[LambdaCutoff_] := mA2 + dmA2/LambdaCutoff^2;
mH2[LambdaCutoff_] := mH2 + dmH2/LambdaCutoff^2;
mh2[LambdaCutoff_] := mh2 + dmh2/LambdaCutoff^2;

mAH2[LambdaCutoff_] := mAH2 + dmAH2/LambdaCutoff^2;
mAh2[LambdaCutoff_] := mAh2 + dmAh2/LambdaCutoff^2;

