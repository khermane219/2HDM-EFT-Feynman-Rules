(* Mass eigenvalues for 3x3 *)
(* Exact expression needed if one needs to find the dimension-six contributions to the masses *)

(* b3 = |b|, p3 = |p|, and q3 = |q| are written in absolute value *)

b3[LambdaCutoff_] := mA2[LambdaCutoff] + mH2[LambdaCutoff] + mh2[LambdaCutoff];
c3[LambdaCutoff_] := mH2[LambdaCutoff]*mh2[LambdaCutoff] + mA2[LambdaCutoff]*(mH2[LambdaCutoff] + mh2[LambdaCutoff]) - mAH2[LambdaCutoff]^2 - mAh2[LambdaCutoff]^2;
d3[LambdaCutoff_] := mAH2[LambdaCutoff]^2*mh2[LambdaCutoff] + mAh2[LambdaCutoff]^2*mH2[LambdaCutoff] - mA2[LambdaCutoff]*mH2[LambdaCutoff]*mh2[LambdaCutoff];

p3[LambdaCutoff_] := 1/9*(b3[LambdaCutoff]^2 - 3*c3[LambdaCutoff]);
q3[LambdaCutoff_] := 1/27*(2*b3[LambdaCutoff]^3 - 9 b3[LambdaCutoff]*c3[LambdaCutoff] + 27*d3[LambdaCutoff]);

zeta[LambdaCutoff_]  := 1/2*(q3[LambdaCutoff] + I*Sqrt[4*p3[LambdaCutoff]^3 - q3[LambdaCutoff]^2]);
sigma[LambdaCutoff_] := 1/2*(q3[LambdaCutoff] - I*Sqrt[4*p3[LambdaCutoff]^3 - q3[LambdaCutoff]^2]);

mh12[LambdaCutoff_] := mh12 + dmh12/LambdaCutoff^2;
mh22[LambdaCutoff_] := mh22 + dmh22/LambdaCutoff^2;
mh32[LambdaCutoff_] := mh32 + dmh32/LambdaCutoff^2;

(*
